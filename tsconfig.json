{
    "compilerOptions": {
        "target": "ES2020",
        "module": "Node16",
        "moduleResolution": "Node16",
        "outDir": "./dist",
        "lib": ["ES2020", "DOM"],
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "types": ["node", "obsidian"]
    },
    "include": ["main.ts", "src"]
}